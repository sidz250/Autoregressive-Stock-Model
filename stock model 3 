   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import statsmodels.api as sm\n",
    "data_path = '/Users/siddhanthnarayanan/Downloads/Code for FINE 3310 group assignment.csv'\n",
    "t_data = pd.read_csv(data_path)\n",
    "y = t_data.RET\n",
    "x=t_data.sprtrn\n",
    "x = sm.add_constant(x)\n",
    "est=sm.OLS(y, x)\n",
    "est = est.fit()\n",
    "est.summary()\n"
   ]
